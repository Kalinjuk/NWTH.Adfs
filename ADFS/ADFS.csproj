<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>nwth.ADFS</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.1.0.24</Version>
    <Company>NWTH</Company>
    <AssemblyVersion>1.1.0.24</AssemblyVersion>
    <FileVersion>1.1.0.24</FileVersion>
    <Description>Авторизация в Web приложении при помощи ADFS</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
